{
  "name": "next-starter-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex migrate:latest",
    "db:migrate:undo": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex migrate:down",
    "db:migrate:make": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex migrate:make",
    "vercel-build": "prisma generate && pnpm db:migrate && next build"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@prisma/client": "4.15.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "autoprefixer": "10.4.14",
    "dotenv": "^16.4.5",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.6",
    "knex": "^2.4.2",
    "next": "13.4.6",
    "pg": "^8.11.0",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "prisma": "^4.15.0"
  }
}